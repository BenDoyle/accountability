<h1><%= link_to @contest.region.name, @contest.region %>, <%= @contest.date %></h1> 

<table>
  <tr>
    <th>Party</th>
    <th>Person</th>
    <th>Votes</th>
  </tr>
 
<% @polls.each do |poll| %>
  <tr>
    <td><%= link_to poll.person.party_membership_at(@contest.date).name, poll.person.party_membership_at(@contest.date) %></td>
    <td><%= link_to poll.person.name, poll.person %></td>
    <td><%= poll.votes %></td>
  </tr>
<% end %>
</table>
 
<br />

<%= link_to 'Home', :controller => 'home', :action => 'index' %>
